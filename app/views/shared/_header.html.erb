<header>
    <nav>
      <h1>
      <%= link_to image_tag("logo.png"), root_path%>
    </h1>
      <ul class= "left">
        <li><%= link_to 'All Movie', movies_path %></li>
        <li><%= link_to 'All Users', users_path %></li>

      </ul>
      <ul class="right">
        <% if current_user%>
          <li><%= link_to current_user.email, current_user %> </li>
          <li><%= link_to "Account Settings", edit_user_path(current_user)%></li>
          <li><%= link_to "Sign Out", session_path, data: { turbo_method: :delete}, class: "button"%></li>
        <% else %>
          <li><%= link_to "Sign in", signin_path, class: "button"%></li>
        <li><%= link_to "Sign Up", signup_path, class: "button"%></li>
        <%end %>
      </ul>
    </nav>
    </header>